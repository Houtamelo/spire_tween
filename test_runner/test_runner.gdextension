macos.release = "res://../tests/target/release/libtests.dylib"
macos.debug.arm64 = "res://../tests/target/debug/libtests.dylib"
macos.release.arm64 = "res://../tests/target/release/libtests.dylib"
web.debug.wasm32 = "res://../tests/target/wasm32-unknown-emscripten/debug/tests.wasm"
web.release.wasm32 = "res://../tests/target/wasm32-unknown-emscripten/release/tests.wasm"